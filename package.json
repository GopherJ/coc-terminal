{
  "name": "coc-terminal",
  "version": "0.3.1",
  "description": "Toggle terminal with coc.nvim",
  "author": "Heyward Fann <fannheyward@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "coc.nvim"
  ],
  "engines": {
    "coc": "^0.0.70"
  },
  "scripts": {
    "clean": "rimraf lib",
    "watch": "webpack --watch",
    "build": "webpack",
    "prepare": "npm-run-all clean build"
  },
  "devDependencies": {
    "@types/node": "^13.7.0",
    "coc.nvim": "^0.0.79",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^6.0.4",
    "typescript": "^4.1.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^4.2.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "terminal.Toggle",
        "title": "Toggle terminal showing"
      },
      {
        "command": "terminal.REPL",
        "title": "Create REPL for current file"
      },
      {
        "command": "terminal.Destroy",
        "title": "Destroy and free terminal"
      }
    ]
  }
}
